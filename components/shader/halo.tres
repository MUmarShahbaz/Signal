code = "shader_type canvas_item;

uniform vec4 white_threshold = vec4(0.9, 0.9, 0.9, 0.9);
uniform float multiplier = 2.0;

void fragment() {
	vec4 my_tex = texture(TEXTURE, UV);
	if (!(my_tex.r > white_threshold.r && my_tex.g > white_threshold.g && my_tex.b > white_threshold.b && my_tex.a > white_threshold.a)) {
		COLOR = my_tex * multiplier;
	}
}"

